package testPrimeNumbers;


public class PrimeNumberTester {

	public static void main(String[] args) {
		System.out.println("Prime numbers between 1 and 100 are as below : ");
		for( int i = 1; i <= 100; i++ ) {
			for( int j = 2; j < i; j++ ){
				if( i % j == 0 ) {
					break;
				}
				if( j == i - 1 ) {
					System.out.println(i);
				}
			}
		}

	}

}
